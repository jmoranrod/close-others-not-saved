{
  "name": "close-others-not-saved",
  "displayName": "Close Others Not Saved",
  "icon": "icon.png",
  "description": "Closes all saved (non-dirty) editors in all groups.",
  "publisher": "jmoran",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmoranrod/close-others-not-saved.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "closeOthersNotSaved.execute",
        "title": "Close Others Not Saved"
      },
      {
        "command": "closeOthersNotSaved.closeOthersNotSaved",
        "title": "Close Others Not Saved"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "command": "closeOthersNotSaved.closeOthersNotSaved",
          "group": "2_close@10"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}